{
  "name": "focus-locus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@focus-locus/web",
    "build": "npm run build --workspace=@focus-locus/web",
    "lint": "npm run lint --workspace=@focus-locus/web",
    "preview": "npm run preview --workspace=@focus-locus/web",
    "install-all": "npm install",
    "mobile": "npm start --workspace=@focus-locus/mobile",
    "mobile:android": "npm run android --workspace=@focus-locus/mobile",
    "mobile:ios": "npm run ios --workspace=@focus-locus/mobile"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@tailwindcss/vite": "^4.1.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "^7.6.3",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.0.4"
  }
}
